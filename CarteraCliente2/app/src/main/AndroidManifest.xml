<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.carteracliente">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="CarteraClientes"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CarteraCliente">
        <activity
            android:name=".ActNuevoCliente"
            android:exported="true"
            android:label="@string/title_activity_act_nuevo_cliente"
            android:theme="@style/Theme.CarteraCliente.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ActMain"
            android:exported="true"
            android:label="Cartera Cliente"
            android:theme="@style/Theme.CarteraCliente.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>